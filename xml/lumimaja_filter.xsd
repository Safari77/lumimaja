    <xs:element name="case" type="boolType" minOccurs="1" maxOccurs="1" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="matchSize">
  <xs:sequence>
    <xs:element name="num1" type="xs:int" minOccurs="1" maxOccurs="1" />
